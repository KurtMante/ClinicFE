{
  "version": 2,
  "env": { "CI": "false" },
  "builds": [
    {
      "src": "clinicFE - Copy/clinic/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "cd \"clinicFE - Copy/clinic\" && npm install",
        "buildCommand": "cd \"clinicFE - Copy/clinic\" && npm run build",
        "outputDirectory": "clinicFE - Copy/clinic/build"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/(.*)",
      "dest": "clinicFE - Copy/clinic/build/index.html"
    }
  ]
}